<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\convert_dark.png" />
    <None Remove="Images\convert_light.png" />
    <None Remove="Images\dashboard_dark.png" />
    <None Remove="Images\dashboard_light.png" />
    <None Remove="Images\dollar_light.png" />
    <None Remove="Images\home_dark.png" />
    <None Remove="Images\home_light.png" />
    <None Remove="Images\ic_clear.png" />
    <None Remove="Images\ic_search.png" />
    <None Remove="Images\info_dark.png" />
    <None Remove="Images\info_light.png" />
    <None Remove="Images\settings_dark.png" />
    <None Remove="Images\settings_light.png" />
    <None Remove="Images\up_arrow_light.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\convert_dark.png" />
    <Resource Include="Images\convert_light.png" />
    <Resource Include="Images\dashboard_dark.png" />
    <Resource Include="Images\dashboard_light.png" />
    <Resource Include="Images\dollar_light.png" />
    <Resource Include="Images\home_dark.png" />
    <Resource Include="Images\home_light.png" />
    <Resource Include="Images\ic_clear.png" />
    <Resource Include="Images\ic_search.png" />
    <Resource Include="Images\info_dark.png" />
    <Resource Include="Images\info_light.png" />
    <Resource Include="Images\settings_dark.png" />
    <Resource Include="Images\settings_light.png" />
    <Resource Include="Images\up_arrow_light.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="Prism.Unity" Version="8.1.97" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Helpers\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
